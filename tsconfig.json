  "compilerOptions": {
    "outDir": "dist",
    "target": "es5",
    "sourceMap": false,
    "declaration": false,
    "moduleResolution": "node",
    "rootDir": "src",
    "forceConsistentCasingInFileNames": true,
    "suppressImplicitAnyIndexErrors": true
  },
  "exclude": ["node_modules", "dist"]
}
